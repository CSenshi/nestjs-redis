{
  "name": "@nestjs-redis/kit",
  "version": "0.13.4",
  "license": "MIT",
  "author": "Saba Pochkhua <saba.pochkhua@gmail.com> (https://github.com/CSenshi)",
  "description": "All-in-one convenience package that re-exports every module in the NestJS Redis Toolkit",
  "keywords": [
    "nestjs",
    "redis",
    "kit",
    "node-redis",
    "re-exports",
    "typescript",
    "lock",
    "distributed",
    "multi-connection",
    "client",
    "database",
    "cache",
    "health-check",
    "terminus",
    "monitoring",
    "throttling",
    "throttler",
    "rate-limiting"
  ],
  "homepage": "https://github.com/CSenshi/nestjs-redis/tree/main/packages/kit",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "default": "./src/index.js"
    }
  },
  "files": [
    "src",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "name": "kit",
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dist/packages/kit",
          "tsConfig": "packages/kit/tsconfig.lib.json",
          "packageJson": "packages/kit/package.json",
          "main": "packages/kit/src/index.ts",
          "assets": [
            "packages/kit/*.md",
            "LICENSE"
          ]
        }
      }
    }
  },
  "dependencies": {
    "tslib": "^2.8.0",
    "@nestjs-redis/client": "workspace:*",
    "@nestjs-redis/health-indicator": "workspace:*",
    "@nestjs-redis/throttler-storage": "workspace:*",
    "@nestjs-redis/lock": "workspace:*",
    "@nestjs-redis/socket.io-adapter": "workspace:*"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CSenshi/nestjs-redis",
    "directory": "packages/kit"
  }
}
